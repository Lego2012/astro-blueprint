{
   "extends": "astro/tsconfigs/strict",
   "compilerOptions": {
      "baseUrl": ".",
      "paths": {
         "@/*": [ "src/*" ],
         "@components/*": [ "src/components/*" ],
         "@layouts/*": [ "src/layouts/*" ],
         "@data/*": [ "src/data/*" ],
         "@pages/*": [ "src/pages/*" ],
         "@ui/*": [ "src/components/ui/*" ],
         "@assets/*": [ "src/assets/*" ],
      }
   }
}
